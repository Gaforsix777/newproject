@model WebApplication1.User
@{
    ViewData["Title"] = "Detalles";
}
<h1>Detalles de usuario</h1>
<hr />
<dl class="row">
    <dt class="col-sm-2">Id</dt>
    <dd class="col-sm-10">@Model.Id</dd>
    <dt class="col-sm-2">Nombre</dt>
    <dd class="col-sm-10">@Model.Nombre</dd>
    <dt class="col-sm-2">Email</dt>
    <dd class="col-sm-10">@Model.Email</dd>
    <dt class="col-sm-2">Rol</dt>
    <dd class="col-sm-10">@Model.Rol</dd>
</dl>

<a class="btn btn-warning" asp-action="Edit" asp-route-id="@Model.Id">Editar</a>
<a class="btn btn-secondary" asp-action="Index">Volver</a>
